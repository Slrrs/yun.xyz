local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/vozoid/ui-libraries/main/drawing/void/source.lua"))()

local watermark = library:Watermark("Yun.xyz | v2.0 | By Rival")
-- watermark:Set("Watermark Set")
-- watermark:Hide() -- toggles watermark

local main = library:Load{
    Name = "Yun.xyz | By Rival",
    SizeX = 500,
    SizeY = 350,
    Theme = "Midnight",
    Extension = "json", -- config file extension
    Folder = "Yun.xyz" -- config folder name
}

-- library.Extension = "txt" (config file extension)
